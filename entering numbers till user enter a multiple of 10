import java.util.*;
public class JavaBasics {
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        do{
            System.out.println("Enter your Number:  ");
            int n=sc.nextInt();
            if(n% 10 == 0){
                break;
            }4System.out.println(n);
        }while(true);
    }
}
    
